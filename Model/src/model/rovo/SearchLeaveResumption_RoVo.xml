          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[select             
hdr.LEAVE_REQ_ID,             
hdr.LEAVE_REQ_NO,             
hdr.PER_ABSENCE_ENTRY_ID,             
hdr.LEAVE_REQ_TYPE,            
hdr.EMP_NO,             
hdr.EMPLOYEE_NAME,            
hdr.STRAT_DATE,             
hdr.END_DATE,            
hdr.APPLIED_LEAVE_DAYS,            
hdr.STATUS,            
hdr.FLOW_WITH,            
hdr.FLOW_STATUS,          
hdr.EXTRA_LEAVE_DAYS,          
hdr.TOTAL_LEAVE_DAYS,         
(select ua.user_name from xxsc_user_access ua where ua.user_id=hdr.FLOW_WITH and rownum=1) as next_appr         
            
from             
xxlr_leave_resumption_header hdr            
where        
decode(nvl((select v.role from xxlr_emp_user_userrole_v v where (v.full_name =:BV_USER_NAME or v.PERSON_NUMBER =:BV_USER_NAME) and role in ('MANAGER','LINE_MANAGER') and rownum=1),'EMP'),      
'MANAGER',0,     
'LINE_MANAGER',(select e.employee_id from xxlr_emp_user_userrole_v e where (e.employee_id=hdr.FLOW_WITH or e.employee_id=hdr.EMP_ID) and rownum=1 and (e.full_name =:BV_USER_NAME or e.PERSON_NUMBER =:BV_USER_NAME))    
,'EMP',(select e.employee_id from xxlr_emp_user_userrole_v e where e.employee_id=hdr.EMP_ID and rownum=1)    
)      
IN      
case      
when (select v.role from xxlr_emp_user_userrole_v v where (v.full_name =:BV_USER_NAME or v.PERSON_NUMBER =:BV_USER_NAME) and role='MANAGER' and rownum=1)='MANAGER' then 0      
     
when (select v.role from xxlr_emp_user_userrole_v v where (v.full_name =:BV_USER_NAME or v.PERSON_NUMBER =:BV_USER_NAME) and role='LINE_MANAGER' and rownum=1)='LINE_MANAGER' then       
(select p.employee_id from xxlr_emp_user_userrole_v p where (p.full_name =:BV_USER_NAME or p.PERSON_NUMBER =:BV_USER_NAME) and rownum=1 and (p.employee_id=hdr.FLOW_WITH or p.employee_id=hdr.EMP_ID))     
    
when nvl((select v.role from xxlr_emp_user_userrole_v v where (v.full_name =:BV_USER_NAME or v.PERSON_NUMBER =:BV_USER_NAME) and role='MANAGER' and rownum=1),'EMP')='EMP' then       
(select p.employee_id from xxlr_emp_user_userrole_v p where (p.full_name =:BV_USER_NAME or p.PERSON_NUMBER =:BV_USER_NAME) and rownum=1)     
else 0      
end      
order by LEAVE_REQ_ID desc]]></SQLQuery>
